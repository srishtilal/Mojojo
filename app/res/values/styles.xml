<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="AppTheme.Base" />

    <style name="AppTheme.Base" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>

    </style>

    <style name="btn">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="myCustomTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="android:textColorSecondary">#FFFFFF</item>
    </style>

    <style name="AppThemeCustom" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/note_button</item>
        <item name="colorPrimaryDark">@color/note_button_pressed</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>

    </style>

    <style name="AppTheme.Custom" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/note_button</item>
        <item name="colorPrimaryDark">@color/note_button_pressed</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>

    </style>

    <!-- Feed Theme -->
    <style name="AppThemeAppointments." parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/feedColorPrimary</item>
        <item name="colorPrimaryDark">@color/feedColorPrimaryDark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>

    <style name="AppTheme.Appointments" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/feedColorPrimary</item>
        <item name="colorPrimaryDark">@color/feedColorPrimaryDark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>




</resources>
